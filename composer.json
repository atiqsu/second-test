{
    "name": "atiqur/how-to-create",
    "type": "library",
    "description": "This is a testing package; learning how to create a package installable via composer",
    "keywords": [
        "atiqur",
        "how-to-create"
    ],
    "homepage": "https://github.com/atiqur/how-to-create",
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Atiqur Rahman",
            "email": "atiq.cse.cu0506.su@gmail.com",
            "homepage": "http://atiq.movtorrents.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "atiqur\\how-to-create\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atiqur\\how-to-create\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
